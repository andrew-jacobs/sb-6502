include	../../../MakeDefns

#===============================================================================
# Targets
#-------------------------------------------------------------------------------

TARGET	=	cat.prg

SRCS	=	cat.c

OBJS	=	$(SRCS:.c=.obj)

LIBS	=	-LC ..\..\..\lib\os65.lib

$(TARGET):	$(OBJS)
		$(LD) $(LDFLAGS) -O $@ $(OBJS) $(LIBS)

#-------------------------------------------------------------------------------
	
clean:
	$(RM) $(TARGET)
	$(RM) $(OBJS)
	$(RM) *.lst
	$(RM) *.map
	
#===============================================================================
# Dependencies
#-------------------------------------------------------------------------------
	
cat.obj:	\
			cat.c
